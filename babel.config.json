{
  "presets": [
    "@babel/preset-env",
    "@babel/preset-typescript"
  ],
  "plugins": [
      ["@babel/plugin-proposal-decorators",
      {
        "legacy": true
      }
    ],
    "@babel/plugin-proposal-class-properties",
    ["angularjs-annotate", { "explicitOnly": true }]
  ],
  "ignore": ["node_modules"]
}
